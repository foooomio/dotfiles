[user]
	name = foooomio
	email = foooomio@gmail.com
[core]
	excludesfile = ~/.gitignore
	editor = vim
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
[push]
	default = simple
[alias]
	co = checkout
[branch]
	autoSetupMerge = false
[pull]
	rebase = false
[init]
	defaultBranch = master
